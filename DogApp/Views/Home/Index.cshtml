@model IEnumerable<DogApp.Models.Dog>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Home Page</h1>

    @foreach (var dog in Model)
    {
        <div>
            <a href="/home/details/@dog.Id">@dog.Name</a>
        </div>
    }
</body>
</html>
